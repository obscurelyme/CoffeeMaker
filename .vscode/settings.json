{
  "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
  "C_Cpp.clang_format_path": "${workspaceDirectory}/.clang-format",
  "C_Cpp.formatting": "clangFormat",
  "files.associations": {
    "*.cpp": "cpp",
    "*.tsx": "typescriptreact",
    "filesystem": "cpp",
    "algorithm": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "cctype": "cpp",
    "charconv": "cpp",
    "chrono": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "condition_variable": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "exception": "cpp",
    "format": "cpp",
    "forward_list": "cpp",
    "fstream": "cpp",
    "functional": "cpp",
    "hash_map": "cpp",
    "hash_set": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "ios": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "iterator": "cpp",
    "limits": "cpp",
    "list": "cpp",
    "locale": "cpp",
    "map": "cpp",
    "memory": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "optional": "cpp",
    "ostream": "cpp",
    "ratio": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "stop_token": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "system_error": "cpp",
    "thread": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "utility": "cpp",
    "vector": "cpp",
    "xfacet": "cpp",
    "xhash": "cpp",
    "xiosbase": "cpp",
    "xlocale": "cpp",
    "xlocbuf": "cpp",
    "xlocinfo": "cpp",
    "xlocmes": "cpp",
    "xlocmon": "cpp",
    "xlocnum": "cpp",
    "xloctime": "cpp",
    "xmemory": "cpp",
    "xstddef": "cpp",
    "xstring": "cpp",
    "xtr1common": "cpp",
    "xtree": "cpp",
    "xutility": "cpp",
    "__config": "cpp",
    "__nullptr": "cpp",
    "deque": "cpp",
    "set": "cpp",
    "strstream": "cpp",
    "__string": "cpp",
    "__node_handle": "cpp",
    "__split_buffer": "cpp",
    "stack": "cpp",
    "string_view": "cpp",
    "bitset": "cpp",
    "queue": "cpp",
    "random": "cpp",
    "*.tcc": "cpp",
    "codecvt": "cpp",
    "cwctype": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "numbers": "cpp",
    "semaphore": "cpp",
    "cinttypes": "cpp",
    "any": "cpp",
    "__bit_reference": "cpp",
    "__debug": "cpp",
    "__errc": "cpp",
    "__functional_base": "cpp",
    "__hash_table": "cpp",
    "__locale": "cpp",
    "__mutex_base": "cpp",
    "__threading_support": "cpp",
    "__tuple": "cpp",
    "__bits": "cpp",
    "__tree": "cpp",
    "complex": "cpp",
    "future": "cpp",
    "source_location": "cpp",
    "__functional_03": "cpp",
    "coroutine": "cpp",
    "variant": "cpp"
  },
  "cmake.configureSettings": {
    "CMAKE_TOOLCHAIN_FILE": "~/vcpkg/scripts/buildsystems/vcpkg.cmake"
  },
  "[cpp]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "clang-tidy.checks": ["clang-analyzer-*"],
  "clang-tidy.compilerArgs": [
    "-Ibuild/vcpkg_installed/x64-osx/include",
    "-Iinclude",
    "-std=c++20"
  ],
  "clang-tidy.blacklist": [
    "build/vcpkg_installed/x64-osx/*"
  ],
  "c-cpp-flylint.clang.enable": false,
  "c-cpp-flylint.flawfinder.enable": false,
  "c-cpp-flylint.lizard.enable": false,
  "c-cpp-flylint.flexelint.enable": false,
  "c-cpp-flylint.cppcheck.enable": true,
  "c-cpp-flylint.cppcheck.standard": ["c++20"]
}
